.m_objects_new
  padding-bottom 35px
  .m_feed-form
    margin-top 20px
    margin-bottom 22px
  .objects-lists
    min-height 200px
    .mpc_preloader
      opacity 0
      &.show
        opacity 1
    .list
      display none
      &.current
        display block
      .col-xs-12
        display none
        @media screen and (min-width: 992px)
          display block
        &:nth-child(1),
        &:nth-child(2),
        &:nth-child(3),
        &:nth-child(4)
          display block